{
    "name": "thomasruiz/larapie",
    "description": "Expose your models through an api in a minute!",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Ruiz",
            "email": "thomas.ruiz@neoxia.com"
        }
    ],
    "require": {
        "illuminate/config": "~5.1",
        "illuminate/routing": "~5.1",
        "illuminate/support": "~5.1",
        "illuminate/database": "~5.1",
        "illuminate/validation": "~5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.4",
        "mockery/mockery": "^0.9.5"
    },
    "autoload": {
        "psr-4": {
            "Larapie\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LarapieTests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Larapie\\LarapieServiceProvider"
            ]
        }
    }
}
